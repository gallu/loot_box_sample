{
    "name": "gallu/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid business use development",
    "keywords": ["microframework", "rest", "psr7", "slim"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michiaki Furusho"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.23",
        "twig/twig": "^2.11",
        "gallu/slim-littie-tools": "^v0.0.32"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
